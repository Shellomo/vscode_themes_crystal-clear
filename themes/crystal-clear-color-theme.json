{
  "name": "Crystal Clear",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1b26",
    "editor.foreground": "#d4d4d4",
    "activityBar.background": "#161721",
    "activityBar.foreground": "#7aa2f7",
    "activityBarBadge.background": "#7aa2f7",
    "activityBarBadge.foreground": "#161721",
    "sideBar.background": "#161721",
    "sideBar.foreground": "#a9b1d6",
    "sideBarTitle.foreground": "#7aa2f7",
    "statusBar.background": "#161721",
    "statusBar.foreground": "#a9b1d6",
    "titleBar.activeBackground": "#161721",
    "titleBar.activeForeground": "#7aa2f7",
    "tab.activeBackground": "#1a1b26",
    "tab.inactiveBackground": "#161721",
    "tab.activeForeground": "#7aa2f7",
    "tab.inactiveForeground": "#545c7e",
    "editorGroupHeader.tabsBackground": "#161721",
    "list.activeSelectionBackground": "#2f334d",
    "list.activeSelectionForeground": "#7aa2f7",
    "list.hoverBackground": "#292e42",
    "input.background": "#1a1b26",
    "input.foreground": "#d4d4d4",
    "input.border": "#292e42",
    "button.background": "#7aa2f7",
    "button.foreground": "#161721"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#565f89"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#9d7cd8"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted.double.html"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#e0af68"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attributes"
      ],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "HTML Attribute Values",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#9ece6a"
      }
    },
    {
      "name": "HTML Character Entities",
      "scope": [
        "constant.character.entity.html",
        "constant.character.entity.named.html",
        "constant.character.entity.numeric.html"
      ],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "HTML Comments",
      "scope": [
        "comment.block.html",
        "punctuation.definition.comment.html"
      ],
      "settings": {
        "foreground": "#565f89"
      }
    },
    {
      "name": "HTML Doctype",
      "scope": [
        "meta.tag.metadata.doctype.html",
        "entity.name.tag.doctype.html",
        "variable.language.doctype.html"
      ],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "HTML ID and Class attributes",
      "scope": [
        "meta.attribute.id.html",
        "meta.attribute.class.html"
      ],
      "settings": {
        "foreground": "#f7768e"
      }
    },
    {
      "name": "HTML Content",
      "scope": [
        "text.html"
      ],
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "Template Languages in HTML (like EJS, Handlebars)",
      "scope": [
        "text.html.derivative",
        "meta.embedded.block",
        "meta.embedded.line"
      ],
      "settings": {
        "foreground": "#89ddff"
      }
    }
  ]
}